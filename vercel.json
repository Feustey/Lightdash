{
  "version": 2,
  "builds": [
    {
      "src": "frontend/build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static",
        "buildCommand": "cd frontend && ./build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/static/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ]
} 