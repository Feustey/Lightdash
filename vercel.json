{
  "version": 2,
  "builds": [
    {
      "src": "frontend/build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/static/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ],
  "env": {
    "SPARKSEER_API_KEY": "@sparkseer-api-key",
    "NODE_PUBKEY": "@node-pubkey",
    "API_URL": "@api-url",
    "ML_URL": "@ml-url"
  }
} 