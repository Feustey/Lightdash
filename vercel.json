{
  "version": 2,
  "buildCommand": "./install.sh && cd frontend && trunk build --release",
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/*.rs": {
      "runtime": "@vercel/rust@4.0.0"
    }
  }
} 